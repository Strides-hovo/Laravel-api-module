{
  "name": "strides/laravel-api-module",
  "description": "A module management system for Laravel APIs",
  "minimum-stability": "stable",
  "version": "1.1.2",
  "keywords": [
    "modules",
    "laravel",
    "strides",
    "module"
  ],
  "license": "MIT",
    "authors": [
        {
            "name": "Strides-hovo",
            "email": "strides@internet.ru",
            "role": "Developer"
        }
    ],
  "autoload": {
    "psr-4": {
      "Strides\\Module\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Strides\\Module\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.17",
    "phpunit/phpunit": "^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Strides\\Module\\Providers\\ModuleServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests"
  },
  "config": {
    "sort-packages": true
  }
}
